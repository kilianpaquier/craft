# Code generated by craft; DO NOT EDIT.

sonar.host.url=https://sonarcloud.io
sonar.qualitygate.wait=true

<<- $maintainer := index .Maintainers 0 >>

sonar.organization=<< $maintainer.Name >>
sonar.projectBaseDir=.
sonar.projectKey=<< print $maintainer.Name "_" .ProjectName >>
sonar.projectName=<< .ProjectName >>

sonar.exclusions=node_modules/**,dist/**,**/*.spec.ts,**/*.test.js
sonar.test.inclusions=**/*.spec.ts,**/*.test.js

<<- if eq .CI.Name "gitlab" >>

sonar.testExecutionReportPaths=reports/node-test.sonar.xml
<<- end >>
sonar.eslint.reportPaths=reports/node-lint.xslint.json
sonar.javascript.lcov.reportPaths=reports/lcov.info